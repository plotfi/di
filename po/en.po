msgid  "di ver %s    Default Format: %s\n"
msgstr "di ver %s    Default Format: %s\n"
msgid  "Usage: di [-ant] [-f format] [-s sort-type] [-i ignore-fstyp-list]\n"
msgstr "Usage: di [-ant] [-f format] [-s sort-type] [-i ignore-fstyp-list]\n"
msgid  "       [-I include-fstyp-list] [-w kbyte-width] [-W inode-width] [file [...]]\n"
msgstr "       [-I include-fstyp-list] [-w kbyte-width] [-W inode-width] [file [...]]\n"
msgid  "   -a   : print all mounted devices\n"
msgstr "   -a   : print all mounted devices\n"
msgid  "   -d x : size to print blocks in (p - posix (512), k - kbytes,\n"
msgstr "   -d x : size to print blocks in (p - posix (512), k - kbytes,\n"
msgid  "          m - megabytes, g - gigabytes, <x> - numeric size).\n"
msgstr "          m - megabytes, g - gigabytes, <x> - numeric size).\n"
msgid  "   -f x : use format string <x>\n"
msgstr "   -f x : use format string <x>\n"
msgid  "   -i x : ignore file system types in <x>\n"
msgstr "   -i x : ignore file system types in <x>\n"
msgid  "   -I x : include only file system types in <x>\n"
msgstr "   -I x : include only file system types in <x>\n"
msgid  "   -l   : display local filesystems only\n"
msgstr "   -l   : display local filesystems only\n"
msgid  "   -n   : don't print header\n"
msgstr "   -n   : don't print header\n"
msgid  "   -s t : sort type; n - no sort, s - by special device, r - reverse\n"
msgstr "   -s t : sort type; n - no sort, s - by special device, r - reverse\n"
msgid  "   -t   : print totals\n"
msgstr "   -t   : print totals\n"
msgid  "   -w n : use width <n> for kbytes\n"
msgstr "   -w n : use width <n> for kbytes\n"
msgid  "   -W n : use width <n> for i-nodes\n"
msgstr "   -W n : use width <n> for i-nodes\n"
msgid  " Format string values:\n"
msgstr " Format string values:\n"
msgid  "    m - mount point                     M - mount point, full length\n"
msgstr "    m - mount point                     M - mount point, full length\n"
msgid  "    b - total kbytes                    B - kbytes available for use\n"
msgstr "    b - total kbytes                    B - kbytes available for use\n"
msgid  "    u - used kbytes                     c - calculated kbytes in use\n"
msgstr "    u - used kbytes                     c - calculated kbytes in use\n"
msgid  "    f - kbytes free                     v - kbytes available\n"
msgstr "    f - kbytes free                     v - kbytes available\n"
msgid  "    p - percentage not avail. for use   1 - percentage used\n"
msgstr "    p - percentage not avail. for use   1 - percentage used\n"
msgid  "    2 - percentage of user-available space in use.\n"
msgstr "    2 - percentage of user-available space in use.\n"
msgid  "    i - total file slots (i-nodes)      U - used file slots\n"
msgstr "    i - total file slots (i-nodes)      U - used file slots\n"
msgid  "    F - free file slots                 P - percentage file slots used\n"
msgstr "    F - free file slots                 P - percentage file slots used\n"
msgid  "    s - filesystem name                 S - filesystem name, full length\n"
msgstr "    s - filesystem name                 S - filesystem name, full length\n"
msgid  "    t - disk partition type             T - partition type, full length\n"
msgstr "    t - disk partition type             T - partition type, full length\n"
