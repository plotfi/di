option-file ../dioptions.dat

loadunit env-main
loadunit env-systype
loadunit env-cc
loadunit env-msgfmt
loadunit env-extension

output  di.env

system type
system rev
system arch
cc
using_gcc
cflags
addcflag -Wextra
addcflag -Wno-unused-but-set-variable
addcflag -Wno-unused-parameter
addcflag -Wno-unknown-warning-option
ldflags
libs
extension obj
extension exe
cmd_msgfmt
